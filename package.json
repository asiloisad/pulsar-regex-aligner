{
  "name": "regex-aligner",
  "description": "Align text using regular expressions or align multiple cursors",
  "repository": "https://github.com/asiloisad/pulsar-regex-aligner.git",
  "keywords": [
    "regex",
    "aligner",
    "tabularize"
  ],
  "version": "1.1.2",
  "license": "MIT",
  "engines": {
    "atom": ">=1.100.0 <2.0.0"
  },
  "main": "./lib/main",
  "activationCommands": {
    "atom-text-editor:not([mini])": [
      "regex-aligner:toggle",
      "regex-aligner:simple"
    ]
  }
}
